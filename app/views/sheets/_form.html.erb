<%= simple_form_for @sheet do |f| %>
  <%= f.error_notification %>

  <%= f.input :title %>


  <%= f.label :instruments %>
  <br>
  <%= f.input_field :instruments, collection: Sheet.values_for_instruments, prompt: "Choose instruments", multiple: true , id: "instruments-field"%>

  <%= f.input :description %>
  <%= f.input :pages %>


  <%= f.button :submit %>
<% end %>