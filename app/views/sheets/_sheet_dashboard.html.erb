<tr>
  <td class="sheet dashboard-sheet">
    <a href="<%= sheet_url(sheet) %>">
    <div class="sheet-text">
      <% if sheet.privately_visible?%>
        <span class="fa fa-eye-slash pull-right"></span>
      <% end %>
      <% unless sheet.cached_votes_up.zero? %>
      <span class="pull-right sheet-star"><%= sheet.cached_votes_up %> <i class="fa fa-star-o"></i></span>
      <% end %>
      <p class="sheet-instruments"><%= sheet.instruments.join(", ") %></p>
      <h3><%= sheet.title %></h3>
      <p><%= sheet.composer_list.any? ? sheet.composer_list.to_s.titleize : "Unknown Composer" %></p>
    </div>
    </a>
  </td>
  <td>
    <div class="form-group">
      <input id="sheet-direct-url" onClick="this.select();" class="form-control" readonly value="<%= sheet_url(sheet) %>">
    </div>
    <%= render 'sheets/share_buttons', sheet: sheet %>
  </td>
  <td>
    <a href="<%= edit_sheet_path(sheet) %>">
      <button type="button" class="btn btn-success btn-block"><i class="fa fa-edit"></i> Edit</button>
    </a>
  </td>
</tr>