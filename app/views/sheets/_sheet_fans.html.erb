<% if likes.present? %>
<li class="list-group-item">
      <% likes.each do |like| %>
        <%= render 'fan', fan: like.voter %>
      <% end %>
      <a href="<%= fans_sheet_url(sheet) %>" class="bg-info" data-toggle="tooltip" data-placement="top" title="View All <%= sheet.cached_votes_up %> Fans"><em class="fa fa-plus"></em></a>
</li>
<% end %>